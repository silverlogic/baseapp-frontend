    "@storybook/test": "8.4.7",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/color": "^4.2.0",
    "@types/jest": "29.5.12",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.7",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "autoprefixer": "10.4.19",
    "babel-jest": "29.7.0",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "css-loader": "7.1.2",
    "eslint-plugin-storybook": "^0.8.0",
    "execa": "^9.5.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.38",
    "postcss-loader": "8.1.1",
    "storybook": "8.4.7",
    "style-loader": "4.0.0",
    "tailwindcss": "3.4.4",
    "ts-jest": "29.1.4",
    "ts-node": "10.9.2",
    "tsup": "8.3.6",
    "typescript": "5.8.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
