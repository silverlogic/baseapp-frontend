{
  "name": "@baseapp-frontend/components",
  "description": "BaseApp components modules such as comments, notifications, messages, and more.",
  "version": "1.3.0",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && pnpm relay && tsc --build tsconfig.build.json",
    "dev": "rm -rf dist && tsc --watch",
    "relay": "relay-compiler",
    "relay:download-schema": "dotenv -- bash -c 'get-graphql-schema \"$NEXT_PUBLIC_RELAY_ENDPOINT\" > schema.graphql'",
    "relay:update-schema": "pnpm relay:download-schema && pnpm relay",
    "lint": "eslint . --ext .tsx --ext .ts && tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "prettier": "prettier --write .",
    "storybook": "pnpm relay && storybook dev -p 6007",
    "storybook:build": "storybook build",
    "cypress:clean": "rm -rf ./cypress/downloads ./cypress/screenshots ./cypress/videos",
    "cypress:open": "cypress open --browser chrome",
    "test:component": "pnpm relay && cypress run --component --browser chrome --headed",
    "test:unit": "jest --config ./jest.config.ts",
    "prepare": "pnpm build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./shared/common": "./modules/__shared__/common/index.ts",
    "./shared/web": "./modules/__shared__/web/index.ts",
    "./shared/native": "./modules/__shared__/native/index.ts",
    "./activity-log/common": "./modules/activity-log/common/index.ts",
    "./activity-log/web": "./modules/activity-log/web/index.ts",
    "./activity-log/native": "./modules/activity-log/native/index.ts",
    "./comments/common": "./modules/comments/common/index.ts",
    "./comments/web": "./modules/comments/web/index.ts",
    "./comments/native": "./modules/comments/native/index.ts",
    "./messages/common": "./modules/messages/common/index.ts",
    "./messages/web": "./modules/messages/web/index.ts",
    "./messages/native": "./modules/messages/native/index.ts",
    "./navigations/common": "./modules/navigations/common/index.ts",
    "./navigations/web": "./modules/navigations/web/index.ts",
    "./navigations/native": "./modules/navigations/native/index.ts",
    "./notifications/common": "./modules/notifications/common/index.ts",
    "./notifications/web": "./modules/notifications/web/index.ts",
    "./notifications/native": "./modules/notifications/native/index.ts",
    "./profiles/common": "./modules/profiles/common/index.ts",
    "./profiles/web": "./modules/profiles/web/index.ts",
    "./profiles/native": "./modules/profiles/native/index.ts",
    "./tests/common": "./modules/tests/common/index.ts",
    "./tests/web": "./modules/tests/web/index.ts",
    "./tests/native": "./modules/tests/native/index.ts",
    "./content-feed/common": "./modules/content-feed/common/index.ts",
    "./content-feed/web": "./modules/content-feed/web/index.ts",
    "./content-feed/native": "./modules/content-feed/native/index.ts"
  },
  "files": [
    "modules",
    "tsconfig.json",
    "relay.config.js",
    "schema.graphql",
    "__generated__"
  ],
  "dependencies": {
    "@emotion/cache": "11.14.0",
